<div class="container"
  <div class="jumbotron">
    <h1>Nearest Alt Fuel Stations</h1>
    <% @stations.each do |station| %>
      <h3><%= station.station_name %></h3>
      <p>Address: <%= station.street_address %></p>
      <p>Fuel Types: <%= station.fuel_type_code %></p>
      <p>Access Times: <%= station.access_days_time %></p>
      <p>Distance: <%= station.distance %></p>
      <br>
    <% end %>
  </div>
</div>
